{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./static/*"]
    },
    "outDir": "./static/dist"
  },
  "include": ["static/JS/Components/**/*"],
  "exclude": ["node_modules"]
}